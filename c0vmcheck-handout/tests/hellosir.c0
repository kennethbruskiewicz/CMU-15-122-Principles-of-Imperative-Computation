#use <string>
string f(string name){
  name = "NO";
  return name;
}
int main () {
  print("What's your name? ");
  string name = readline();
  print(string_join("Hello, ", string_join(name, "!\n")));
  string x = f(name);
  print(string_join("Hello, ", string_join(x, "!\n")));
  return 0;
}
